# .env.example
# Renomeie este arquivo para .env e preencha com seus valores

# Configurações das API Keys
EVOLUX_OPENROUTER_API_KEY="your-openrouter-api-key-here"
EVOLUX_OPENAI_API_KEY="your-openai-api-key-here"  
EVOLUX_GOOGLE_API_KEY="your-google-api-key-here"

# Configurações opcionais
EVOLUX_HTTP_REFERER="https://your-domain.com"
EVOLUX_X_TITLE="Evolux Engine"
EVOLUX_DEVELOPMENT_MODE=false
EVOLUX_DEBUG_MODE=false

